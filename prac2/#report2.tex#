\section{’£²’¼¡’´Ø’¿ô’¤Î’²ò’¤ò’µá’¤á’¤ë’¥×’¥í’¥°’¥é’¥à}
’¥×’¥í’¥°’¥é’¥à’¤Î’¥½’¡¼’¥¹’¤È’¼Â’¹Ô’·ë’²Ì’¤ò’°Ê’²¼’¤Ë’¼¨’¤¹’¡£
\footnotesize
\begin{itembox}[l]{’¥½’¡¼’¥¹}
\begin{verbatim}
/* 5.c */
#include <stdio.h>

int main()
{
  int a, b, c, x, y;

  printf("a: ");
  scanf("%d",&a);

  printf("b: ");
  scanf("%d", &b);

  printf("c: ");
  scanf("%d",&c);

  printf(" y = ax*x + bx + c = (%d)x*x + (%d)x + (%d)\n", a, b, c);

  printf("x: ");
  scanf("%d", &x);

  /* y ’¤ò’µá’¤á’¤ë */
  y =  a*x*x + b*x + c;
  
  printf(" y = %d, if x = %d.\n", y, x);

  return 0;
}
\end{verbatim}
\end{itembox}

\begin{itembox}[l]{’¼Â’¹Ô’·ë’²Ì}
\begin{verbatim}
[~/report/softB/prac2] $ ./a.out 
a: -1
b: 6
c: -4
 y = ax*x + bx + c = (-1)x*x + (6)x + (-4)
x: 3
 y = 5, if x = 3.
\end{verbatim}
\end{itembox}

