\section{’ÏÂ’¤ò’µá’¤á’¤ë’¥×’¥í’¥°’¥é’¥à}
’¥×’¥í’¥°’¥é’¥à’¤Î’¥½’¡¼’¥¹’¤È’¼Â’¹Ô’·ë’²Ì’¤ò’°Ê’²¼’¤Ë’¼¨’¤¹’¡£
\footnotesize
\begin{itembox}[l]{’¥½’¡¼’¥¹}
\begin{verbatim}
/* 2.c */
#include <stdio.h>

int main()
{
  int a, b;

  printf("a: ");
  scanf("%d",&a);

  printf("b: ");
  scanf("%d", &b);

  printf(" a = %d, b = %d\n", a, b);
  printf(" a + b = %d\n", a + b);

  return 0;
}
\end{verbatim}
\end{itembox}

\begin{itembox}[l]{’¼Â’¹Ô’·ë’²Ì}
\begin{verbatim}
[~/report/softB/prac2] $cc -o 2 2.c
[~/report/softB/prac2] $./2 
a: 5
b: 7
 a = 5, b = 7
 a + b = 12
\end{verbatim}
\end{itembox}


