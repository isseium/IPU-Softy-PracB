\section{’Å¸’³«’¤ò’¹Ô’¤¦’¥×’¥í’¥°’¥é’¥à}
’¥×’¥í’¥°’¥é’¥à’¤Î’¥½’¡¼’¥¹’¤È’¼Â’¹Ô’·ë’²Ì’¤ò’°Ê’²¼’¤Ë’¼¨’¤¹’¡£
\footnotesize
\begin{itembox}[l]{’¥½’¡¼’¥¹}
\begin{verbatim}
/* 4.c */
#include <stdio.h>

int main()
{
  int a, b, c, d;

  printf("(ax+b)(cx+d)\n");

  printf("a: ");
  scanf("%d",&a);

  printf("b: ");
  scanf("%d", &b);

  printf("c: ");
  scanf("%d",&c);

  printf("d: ");
  scanf("%d", &d);


  printf(" (%d)*x*x+(%d)*x+(%d)\n",a*c, a*d+b*c, b*d);

  return 0;
}
\end{verbatim}
\end{itembox}

\begin{itembox}[l]{’¼Â’¹Ô’·ë’²Ì}
\begin{verbatim}
