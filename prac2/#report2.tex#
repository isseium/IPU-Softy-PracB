\begin{itembox}[l]{’¥½’¡¼’¥¹}
\begin{verbatim}
/* 3.c */
#include <stdio.h>
int main()
{
  int a, b;

  printf("a: ");
  scanf("%d",&a);

  printf("b: ");
  scanf("%d", &b);

  printf(" a = %d, b = %d\n", a, b);
  if ( b != 0 )                 /* 0 ’¤Ç’³ä’¤ë’¤È’¤¤’¤¦’¥¨’¥é’¡¼’¤ò’ËÉ’»ß */
    printf(" a / b = %d ... %d\n", a / b, a % b);
  else
    printf("0 ’¤Ç’¤Ï’³ä’¤ì’¤Þ’¤»’¤ó\n");
    
  return 0;
}
\end{verbatim}
\end{itembox}

\begin{itembox}[l]{’¼Â’¹Ô’·ë’²Ì}
